<?xml version="1.0" encoding="utf-8" ?>
        <!ELEMENT dataset (MAP*, GAME*, CAR*, MOVEMENT_HISTORY*)>

        <!--GameEntity-->
        <!ELEMENT GAME EMPTY>
        <!ATTLIST GAME ID CDATA #REQUIRED>
        <!ATTLIST GAME NAME CDATA #REQUIRED>
        <!ATTLIST GAME MAP_ID CDATA #REQUIRED>

        <!--CarEntity-->
        <!ELEMENT CAR EMPTY>
        <!ATTLIST CAR ID CDATA #REQUIRED>
        <!ATTLIST CAR NAME CDATA #REQUIRED>
        <!ATTLIST CAR GAME_ID CDATA #IMPLIED>

        <!--MapEntity-->
        <!ELEMENT MAP EMPTY>
        <!ATTLIST MAP ID CDATA #REQUIRED>
        <!ATTLIST MAP NAME CDATA #REQUIRED>
        <!ATTLIST MAP ACTIVE CDATA #REQUIRED>
        <!ATTLIST MAP CONTENT CDATA #REQUIRED>

        <!--MoveHistoryEntity-->
        <!ELEMENT MOVEMENT_HISTORY EMPTY>
        <!ATTLIST MOVEMENT_HISTORY ID CDATA #REQUIRED>
        <!ATTLIST MOVEMENT_HISTORY CAR_ID CDATA #REQUIRED>
        <!ATTLIST MOVEMENT_HISTORY GAME_ID CDATA #REQUIRED>
        <!ATTLIST MOVEMENT_HISTORY NEW_POSITION_X CDATA #REQUIRED>
        <!ATTLIST MOVEMENT_HISTORY NEW_POSITION_Y CDATA #REQUIRED>
        <!ATTLIST MOVEMENT_HISTORY NEW_DIRECTION CDATA #REQUIRED>